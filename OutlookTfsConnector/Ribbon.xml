<?xml version="1.0" encoding="UTF-8"?>
<customUI onLoad="Ribbon_Load" xmlns="http://schemas.microsoft.com/office/2006/01/customui">
    <ribbon>
        <officeMenu>
            <button id="button3" imageMso="SetAsDefault" onAction="btnSettings_Click" screentip="Settings" label="TFS Addin Settings" />
        </officeMenu>
        <tabs>
            <tab idMso="TabMail">
                <group id="groupTabMailExplorer" insertBeforeMso="GroupContactFind" label="TFS">
                    <button id="btnAddEmailToTfs" onAction="btnAddEmailToTfs_Click" label="Add Email To TFS" size="large" getImage="getTfsImage" />
                    <button id="btnSettings" imageMso="SetAsDefault" onAction="btnSettings_Click" screentip="Settings" label="Settings" size="large" />
                </group>
            </tab>
            <tab idMso="TabReadMessage">
                <group id="group2" label="TFS">
                    <button id="btnAddEmailToTfs2" onAction="btnAddEmailToTfs_Click" label="Add Email To TFS" size="large" getImage="getTfsImage" />
                </group>
            </tab>
            <tab idMso="TabNewMailMessage">
                <group id="group3" label="TFS">
                    <button id="button1" onAction="btnAddEmailToTfsNewEmail_Click" label="Add Email To TFS" size="large" getImage="getTfsImage"/>
                </group>
            </tab>
            <tab idMso="TabMeetingResponse">
                <group id="group4" label="TFS">
                    <button id="button2" onAction="btnAddEmailToTfs_Click" label="Add Email To TFS" size="large" getImage="getTfsImage"/>
                </group>
            </tab>
        </tabs>
    </ribbon>
</customUI>